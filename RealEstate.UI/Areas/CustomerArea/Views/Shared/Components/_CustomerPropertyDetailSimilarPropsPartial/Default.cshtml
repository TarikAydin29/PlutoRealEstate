@model IEnumerable<RealEstate.Entities.Entities.Property>


<div class="similar-grids">


    <h4>Diğer İlanlarımız</h4>

    @foreach (var item in Model)
    {
        <div class="col-md-4 similar-grid">
            <img src="/Images/Uploads/@item.ImageUrl" class="img-responsive" alt="/">
            <h5>@item.PropertyTitle</h5>
            <p>@item.Description <a asp-area="CustomerArea" asp-controller="Default" asp-action="PropertyDetails" asp-route-id="@item.Id">Know More</a></p>
        </div>
    }

    <div class="clearfix"></div>
</div>