@model IEnumerable<Property>


<div class="slider">
    <div class="callbacks_container">
        <ul class="rslides" id="slider">
            @foreach (var item in Model)
            {
                <div class="slid banner1">
                    <div class="caption">
                        <h3>@item.City @item.County @item.District</h3>
                        <p>@item.Description</p>
                        <a href="/Home/PropertyDetails/@item.Id" class="button">Detaylı Bilgi</a>
                    </div>
                </div>
            }
        </ul>
    </div>
</div>